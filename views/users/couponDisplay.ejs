<%- include ('../layouts/header.ejs') %>

<div class="col-md-6">
    <h3 class="mb-5">Coupons</h3>
    <% let count=1 %>
    
    <table class="table ">
        <thead>
          <tr>
            <th scope="col">name</th>
            <th scope="col">discount</th>
            <th scope="col">minimumvalue</th>
            <th scope="col">maximumvalue</th>
            <th scope="col">expiryDate</th>
             </tr>
        </thead>
        <% if(coupondata.length>0 ){ %>
            <% coupondata.forEach(item=> { %>
        <tbody>
          <tr>
            <th scope="row"><%=count++%></th>
            <td class="fw-bold text-start"><%= item.name %>,<%= item.discount %><br><%= item.minimumvalue %>,<%= item.maximumvalue %>,<%=item.expiryDate %></td>
    <td> <button type="button" class="btn btn-outline-danger btn-sm" id="<%= count%>"
        onclick="deleteAddress(this.id,'<%=item._id %>')"data-url="<%=item._id %>"> Delete </button>
    </td>
            
          </tr>
          </tbody>
          <% }) %>
          <% } %>
          </table> 
        
        
          
           


      </div>



<%- include ('../layouts/footer.ejs') %>